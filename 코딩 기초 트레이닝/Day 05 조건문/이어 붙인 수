class Solution {
    public int solution(int[] num_list) {
        int answer = 0;
        
        String value1 = "";
        String value2 = "";
        
        for(int i=0; i<num_list.length; i++){
            if(num_list[i] % 2 == 0){
                value1 += num_list[i];
            } else{
                value2 += num_list[i];
            }
        }
        
        answer = Integer.parseInt(value1) + Integer.parseInt(value2);
        
        return answer;
    }
}
