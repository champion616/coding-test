import java.util.Arrays;

class Solution {
    public int solution(String[] strArr) {
        int answer = 0;
        
        int[] counts = new int[30];
        for(int i=0; i<strArr.length; i++){
            counts[strArr.length-1]++;
        }
        
        Arrays.sort(strArr);
        answer = counts[29]+1;
        
        return answer;
    }
}
