class Solution {
    public int[] solution(int[] numbers, int num1, int num2) {
        int[] answer = {};
        
        int size = num2 - num1 + 1;
        answer = new int[size];
        
        for(int i=0; i<size; i++){
            answer[i] = numbers[num1++];
        }
        return answer;
    }
}
